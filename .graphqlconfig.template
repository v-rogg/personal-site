{
  "name": "Personal Website GraphQL Schema",
  "schemaPath": "./schema.graphql",
  "extensions": {
    "endpoints": {
      "Fauna GraphQL Endpoint": {
        "url": "https://graphql.eu.fauna.com/graphql",
        "headers": {
